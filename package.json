{
  "name": "@stackbox-dev/fp-plugins",
  "version": "2.11.0",
  "description": "Fastify Plugins used in various stackbox applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:stackbox-dev/fp-plugins.git"
  },
  "scripts": {
    "prepare": "husky",
    "pretty": "prettier --write \"./src/**/*.{js,ts,json,md}\"",
    "clean": "rimraf dist",
    "test": "jest --testPathIgnorePatterns=rabbitmq.spec.ts",
    "test:coverage": "jest --coverage --testPathIgnorePatterns=rabbitmq.spec.ts",
    "test:rabbitmq": "jest rabbitmq.spec.ts --testTimeout=120000",
    "test:all": "jest --testTimeout=120000",
    "transpile": "tsc -p tsconfig.build.json",
    "build": "run-s clean transpile",
    "prepublishOnly": "npm run build"
  },
  "author": "Stackbox <dev@stackbox.xyz>",
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts,json,md}": "prettier --write"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.962.0",
    "@aws-sdk/credential-provider-node": "^3.962.0",
    "@azure/identity": "^4.13.0",
    "@azure/service-bus": "^7.9.5",
    "@azure/storage-blob": "^12.29.1",
    "@google-cloud/pubsub": "^5.2.0",
    "@google-cloud/storage": "^7.18.0",
    "fastify-plugin": "^5.1.0",
    "mnemonist": "^0.40.3",
    "prom-client": "^15.1.3",
    "rabbitmq-client": "^5.0.8"
  },
  "peerDependencies": {
    "fastify": "^3.0.0 || ^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^6.0.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "testcontainers": "^11.11.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
